@model Mind.WebUI.Models.TenantIpNetworkViewModel

@{
    ViewData["Title"] = "Tenant IP Network Details";
}

<ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/Home">Home</a></li>
    <li class="breadcrumb-item"><a href="/Tenant/GetAll">Tenants</a></li>
    <li class="breadcrumb-item"><a href="/TenantIpNetwork/GetAllByTenantID?tenantId=@Model.TenantId">Tenant IP Network - [@Model.TenantName]</a></li>
    <li class="breadcrumb-item active">Details - [@Model.CidrNameIncludingIpv4LessThanOrEqualToLength]</li>
</ol>

<h2>Details for Tenant IP Network '@Model.CidrNameIncludingIpv4LessThanOrEqualToLength'</h2>

@{await Html.RenderPartialAsync("_MessagingPartial");}

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Ipv4Prefix)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Ipv4Prefix)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Ipv4Length)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Ipv4Length)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Ipv4LessThanOrEqualToLength)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Ipv4LessThanOrEqualToLength)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.CidrNameIncludingIpv4LessThanOrEqualToLength)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.CidrNameIncludingIpv4LessThanOrEqualToLength)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.AllowExtranet)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.AllowExtranet)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.IpRoutingBehaviour)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IpRoutingBehaviour)
        </dd>
    </dl>
    </div>
<form asp-action="">
    <div class="form-actions no-color">
        <a asp-action="GetAllByTenantID" asp-route-tenantID="@Model.TenantId">Back to Tenant IP Networks List</a>
    </div>
</form>